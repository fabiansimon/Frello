{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "strictNullChecks": true,
  "include": ["src/**/*", "index.ts", "trpc.ts"],
  "exclude": ["node_modules"]
}
